{
    "makefile.extensionOutputFolder": "./.vscode",
    "files.associations": {
        "*.props": "xml",
        "*.targets": "xml",
        "*.h": "c",
        "*.c": "c",
        "random": "c",
        "algorithm": "c",
        "atomic": "c",
        "sstream": "c",
        "array": "c",
        "string_view": "c",
        "initializer_list": "c"
    },
    "C_Cpp.errorSquiggles": "disabled",
    "[python]": {
        "editor.defaultFormatter": "ms-python.autopep8"
    },
    "python.formatting.provider": "none",
}